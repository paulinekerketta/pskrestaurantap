<p id="notice"><%= notice %></p>

<h1>Cashouts</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Branch</th>
      <th>Salesperson</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cashouts.each do |cashout| %>
      <tr>
        <td><%= cashout.date %></td>
        <td><%= cashout.branch %></td>
        <td><%= cashout.salesperson %></td>
        <td><%= cashout.rate %></td>
        <td><%= link_to 'Show', cashout %></td>
        <td><%= link_to 'Edit', edit_cashout_path(cashout) %></td>
        <td><%= link_to 'Destroy', cashout, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cashout', new_cashout_path %>
